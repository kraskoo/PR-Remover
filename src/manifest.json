{
  "name": "PR Remover",
  "version": "2.0.1",
  "description": "Removes Pop-Up instantly",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "PR Remover"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
